<hr>
<div class="top-100 row justify-content-center">
  <div class="col-md-4">
    <h3 align="center">Top 100 Most Visited</h3>
    <br>
    <% @urls.each.with_index(1) do |url, i| %>
      <div class="row col-md-10 justify-content-center">
        <ol class="ol-100-lists">
          <%= i %>. <%= link_to "localhost:3000/#{url.id.to_s(36)}", url.full_url %>
        </ol>
      </div>
    <% end %>
  </div>
</div>